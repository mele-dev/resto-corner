
# contraseña del usuario root de mysql
MYSQL_ROOT_PASSWORD=Root@12345

# nombre de la base de datos (se crea automáticamente)
MYSQL_DATABASE=CornerAppDb

# backend (api .net 8)
# clave secreta para firmar tokens jwt (mínimo 32 caracteres)
# IMPORTANTE: cambiar este valor en producción
JWT_SECRET_KEY=your-super-secret-key-min-32-characters-long-change-this-in-production

JWT_ISSUER=CornerApp
JWT_AUDIENCE=CornerApp

BACKEND_PORT=8080
FRONTEND_PORT=80

CORS_ORIGIN_0=http://localhost
CORS_ORIGIN_1=http://localhost:3000

REDIS_ENABLED=true

RABBITMQ_ENABLED=true
RABBITMQ_USER=guest
RABBITMQ_PASS=guest

ENABLE_SWAGGER=true
